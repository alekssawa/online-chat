{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "dev": "nodemon --exec tsx src/server.ts",
    "depcheck": "depcheck",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.mongo.prisma",
    "prisma:generate-postgres": "prisma generate --schema=./src/prisma/schema.postgres.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:migrate-all": " prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "prisma:migrate-reset": "prisma migrate reset dev --schema=./src/prisma/schema.prisma",
    "prettier": "prettier --write \"**/*.{ts,tsx,css,js,jsx}\"",
    "tree-cli": "npx tree-cli -l 10 --ignore node_modules",
    "kill-port": "npx kill-port 3000"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express4": "^1.1.2",
    "@prisma/client": "^6.16.1",
    "argon2": "^0.44.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "graphql-upload-minimal": "^1.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.1",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
